struct trivialTree
{
  char letter[1];
  int freq;
};

int main (int argc, char *argv[]) {
  string line;
  char FileName;
  ifstream inMyStream (FileName); 
  int c;
  FileName = argv[2][0];

  if (inMyStream.is_open()) 
    {

      while(  getline (inMyStream, line)){

	cout<<line<<endl;
	c++;
      }
    }
  inMyStream.close(); 

